{
  "name": "zero-ui-cli",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "description": "A cli tool for zero-ui component library.",
  "keywords": [
    "zero-ui",
    "cli",
    "typescript",
    "javascript",
    "js frameworks",
    "component libraries"
  ],
  "author": "Aymen keskas",
  "bin": {
    "zero-ui": "./dist/index.js"
  },
  "maintainers": [
    "Aymen keskas <keskasaymen8@gmail.com> (https://empty-void.vercel.app/)"
  ],
  "files": [
    "./dist/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "settings": {
    "autoInstallPeers": false
  },
  "scripts": {
    "dev": "tsup > /dev/null && node dist/index.js",
    "start": "node dist/index.js",
    "build": "tsup",
    "check": "tsc",
    "release": "tsup && changeset publish",
    "version": "npx changeset"
  },
  "dependencies": {
    "cfonts": "^3.2.0",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.14",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.16",
    "tsup": "^8.0.1",
    "type-fest": "^4.10.2"
  }
}
